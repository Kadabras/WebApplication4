@model UserViewModel


<div>
    <div>
        Id: @Model.Id)
    </div>
    <div>
        Name: @Model.Name)
    </div>
    <div>
        Age: @Model.Age)
    </div>
    <div>
        Email: @Model.Email)
    </div>
    <div>
        Roles: @foreach (var role in Model.Roles)
        {
            <span style="padding:10px">@role  </span>
        }
    </div>

</div>
